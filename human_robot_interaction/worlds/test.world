<actor name="human_94763">
  		<pose>1 1 1.25 0 0 0</pose>

  		<skin>
    		<filename>moonwalk.dae</filename>
    		<scale>1.0</scale>
  		</skin>

  		<animation name="walking">
    		<filename>walk.dae</filename>
    		<scale>1.000000</scale>
    		<interpolate_x>true</interpolate_x>
  		</animation>

  		<plugin name="actor1_plugin" filename="libactor_sub.so">
    		<!-- <target>15 0 1.25</target> -->
    		<target_weight>1.15</target_weight>
    		<obstacle_weight>1.8</obstacle_weight>
    		<animation_factor>5.1</animation_factor>
    		<ignore_obstacles>
      			<model>ground_plane</model>
      	</ignore_obstacles>

  		</plugin>



      <plugin name="attach_model" filename="libAttachModelPlugin.so">
      <link>
        <link_name>human_94763_pose</link_name>
        <model>
          <model_name>human_94763_collision_model</model_name>
        </model>
      </link>
    </plugin>



  </actor>


  </world>
</sdf>