<launch>

 <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find human_robot_interaction)/worlds/worldsim.world"/>
    <!-- more default parameters can be changed here -->
  </include>

  <include file="$(find husky_gazebo)/launch/spawn_husky.launch">
  <arg name="y" value="0.0"/>
  </include>

  <node name="go_to_goal" pkg="human_robot_interaction" type="robot_interaction.py" output="screen"/>
  <node name="human_move" pkg="human_robot_interaction" type="human_move.py"/>
  <!-- <node name="human_behavior_recognition" pkg="human_robot_interaction" type="state_recognition.py" output="screen"/> -->
  
</launch>